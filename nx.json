{
  "npmScope": "nx-workspace",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "forms": {
      "tags": []
    },
    "material": {
      "tags": []
    },
    "matgrid": {
      "tags": []
    },
    "matgrid-e2e": {
      "tags": [],
      "implicitDependencies": ["matgrid"]
    },
    "ngrx": {
      "tags": []
    },
    "ngrx-e2e": {
      "tags": [],
      "implicitDependencies": ["ngrx"]
    }
  }
}
